<div class="col-md-4"></div>
<div class="col-md-3">
  <h2>Fnsh <span class="glyphicon glyphicon-ok main-blue"></span></h2>
  <form action="/session" method="post" role="form">
    <div class="form-group">
      <label for="username">Email:</label> <br>
      <input type="text" name="user[email]" id="email" class="form-control" placeholder="Email" >
    </div>
    <div class="form-group">
      <label for="password">Password:</label> <br>
      <input type="password" name="user[password]" id="password" class="form-control" placeholder="Password">
    </div>
    <div class="form-group">
      <input type="submit" value="Login" class="btn btn-primary btn-small" />
    </div>
    <p>
    Don't have an account? <%= link_to "Sign up!", new_user_path %>
    </p>
  </form>
</div>
<div class="col-md-4"></div>

